MODULE user_command_2000 INPUT.

  CASE gv_ok_code.
    WHEN 'SAVE'.
      CLEAR gv_ok_code.
      PERFORM save.
    WHEN 'BACK'.
      CLEAR gv_ok_code.
      PERFORM back.
  ENDCASE.

ENDMODULE.
